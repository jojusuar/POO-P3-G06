<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vistas.MateriaycursoController">
    <children>
        <Label text="ADMINISTRAR MATERIAS Y PARALELOS" />
      <VBox id="vbMaterias" fx:id="vbMaterias" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label text="TODAS LAS MATERIAS Y PARALELOS" />
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="79.0" prefWidth="212.0" text="(esto es un VBox para mostrarlas)" />
         </children>
      </VBox>
      <Button mnemonicParsing="false" text="Ingresar materia" />
      <Button mnemonicParsing="false" text="Editar materia" />
      <Button mnemonicParsing="false" text="Agregar paralelo" />
      <Button mnemonicParsing="false" text="Editar paralelo" />
        <Button fx:id="leaveMateriaycurso" onAction="#leaveMateriaycurso" text="Volver a Configuraciones" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
