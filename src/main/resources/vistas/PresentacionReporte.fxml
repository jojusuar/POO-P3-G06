<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vistas.PresentacionReporteController">
   <children>
      <BorderPane prefHeight="500.0" prefWidth="700.0">
         <top>
            <Label text="Reporte de Juegos" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets top="30.0" />
               </BorderPane.margin>
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Label>
         </top>
         <bottom>
            <Button fx:id="regresar" mnemonicParsing="false" onAction="#Regresar" text="Regresar" BorderPane.alignment="CENTER_LEFT">
               <BorderPane.margin>
                  <Insets bottom="30.0" left="30.0" />
               </BorderPane.margin>
            </Button>
         </bottom>
         <center>
            <Pane prefHeight="297.0" prefWidth="600.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox fx:id="vbReportes" alignment="TOP_CENTER" layoutX="15.0" layoutY="1.0" prefHeight="380.0" prefWidth="670.0">
                     <opaqueInsets>
                        <Insets left="10.0" />
                     </opaqueInsets></VBox>
               </children></Pane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
