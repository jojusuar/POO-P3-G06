<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vistas.QuestionController">
   <children>
      <VBox alignment="CENTER" layoutX="223.0" layoutY="79.0" prefHeight="200.0" prefWidth="188.0" spacing="10.0">
         <children>
            <Label prefHeight="17.0" prefWidth="154.0" text="ADMINISTRAR PREGUNTAS" />
            <ComboBox id="cbMaterias" fx:id="cbMaterias" prefWidth="150.0" promptText="[Elija una materia]" />
            <Button mnemonicParsing="false" text="Agregar pregunta" />
            <Button mnemonicParsing="false" text="Eliminar pregunta" />
            <Button fx:id="leaveQuestionsButton" mnemonicParsing="false" onAction="#leaveQuestions" text="Volver a Configuraciones" />
         </children>
      </VBox>
   </children>
</AnchorPane>
